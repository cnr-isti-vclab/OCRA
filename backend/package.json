{
  "name": "backend",
  "version": "1.0.0",
  "description": "OAuth session management backend API",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "tsx server.ts",
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "seed": "tsx seed.ts",
