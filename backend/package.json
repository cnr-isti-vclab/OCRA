    "test:quick": "tsx ./scripts/test-log-audit.ts && tsx ./scripts/test-get-users-stats.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "mongodb": "^5.9.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "multer": "^2.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "prisma": "^6.15.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:@types/inflight@^1.0.0",
